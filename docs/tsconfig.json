{
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": true,
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "module": "es2022",
    "noEmit": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["data"],
  "exclude": ["docs/.next", "docs/export", "node_modules"]
}
