{
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": true,
    "moduleResolution": "bundler",
    "jsx": "react",
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["data"],
  "exclude": ["docs/.next", "docs/export", "node_modules"]
}
